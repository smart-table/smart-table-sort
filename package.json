    "smart-table-operators": "^2.0.4"
  },
  "devDependencies": {
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tap-diff": "^0.1.1",
    "xo": "^0.20.3",
    "zora": "^2.0.1"
